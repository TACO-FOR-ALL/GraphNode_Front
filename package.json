{
  "name": "graphnode_front",
  "version": "2.0.0",
  "description": "TACO 4기 GraphNode의 프론트앤드 코드입니다.",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "dist": "vite build && electron-builder",
    "dist:mac:arm": "electron-builder --mac --arm64",
    "dist:mac:intel": "electron-builder --mac --x64",
    "dist:mac:all": "npm run dist:mac:arm && npm run dist:mac:intel",
    "dist:windows": "electron-builder --windows"
  },
  "build": {
    "appId": "com.graphnode.app",
    "asar": false,
    "asarUnpack": [
      "**/node_modules/@floating-ui/**/*",
      "**/node_modules/@tiptap/**/*"
    ],
    "productName": "GraphNode",
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "build/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "hardenedRuntime": true,
      "identity": "YOHAN HAN (GQM6B63P8T)",
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.inherit.plist"
    },
    "afterSign": "scripts/notarize.cjs",
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "GraphNode",
      "uninstallDisplayName": "GraphNode Unintsaller",
      "createDesktopShortcut": "always",
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/d3-force": "^3.0.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.10",
    "electron": "^38.2.2",
    "electron-builder": "^26.7.0",
    "eslint": "^9.37.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.3",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "vite": "^6.3.7",
    "vite-plugin-electron": "^0.29.0"
  },
  "dependencies": {
    "@taco_tsinghua/graphnode-sdk": "^0.1.15",
    "@tanstack/react-query": "^5.90.5",
    "@tiptap/core": "^3.11.0",
    "@tiptap/extension-code-block-lowlight": "^3.11.0",
    "@tiptap/extension-details": "^3.11.0",
    "@tiptap/extension-highlight": "^3.11.0",
    "@tiptap/extension-image": "^3.11.0",
    "@tiptap/extension-mathematics": "^3.11.0",
    "@tiptap/extension-mention": "^3.11.0",
    "@tiptap/extension-table": "^3.11.0",
    "@tiptap/markdown": "^3.11.0",
    "@tiptap/react": "^3.11.0",
    "@tiptap/starter-kit": "^3.11.0",
    "d3-force": "^3.0.0",
    "d3-force-3d": "^3.0.6",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "electron-push-receiver": "^2.1.3",
    "highlight.js": "^11.11.1",
    "i18next": "^25.6.0",
    "keytar": "^7.9.0",
    "lottie-react": "^2.4.1",
    "lowlight": "^3.3.0",
    "openai": "^6.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.0.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.4",
    "remark-gfm": "^4.0.1",
    "three": "^0.181.1",
    "tiptap-markdown": "^0.9.0",
    "zustand": "^5.0.8"
  }
}
